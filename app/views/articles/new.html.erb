<h1> Create a New Form </h1>

<%= form_for :article, url: articles_path do |f|%>
	<p>
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</p>

	<p>
		<%= f.label :text %><br>
		<%= f.text_area :text %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>

<p><%= link_to 'Article List', articles_path %> |

<%= link_to 'Main Page', welcome_index_path %></p>
